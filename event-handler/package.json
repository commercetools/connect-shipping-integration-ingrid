    "axios": "^1.9.0",
    "body-parser": "^2.2.0",
    "dotenv": "^16.5.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "express": "^5.1.0",
    "listr2": "^8.3.3",
    "validator": "^13.15.0"
  }
