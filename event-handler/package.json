  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node build/index.js",
    "gcp-build": "tsc",
    "start:dev": "concurrently -k \"tsc --watch\" \"nodemon -q build/index.js\"",
    "build": "rimraf ./build && tsc",
    "lint": "prettier --check \"**/**/*.{ts,js,json}\" && eslint src test",
    "prettier": "prettier --write '**/*.{js,ts}'",
    "check": "tsc --noEmit && npm run lint",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js jest --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --collectCoverageFrom='src/**/*.ts'",
    "connector:post-deploy": "node build/connector/post-deploy.js",
    "connector:pre-undeploy": "node build/connector/pre-undeploy.js"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@jest/globals": "29.7.0",
    "@tsconfig/recommended": "^1.0.3",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.2.0",
    "@types/node": "^22.15.17",
    "@types/supertest": "^6.0.3",
    "@types/validator": "^13.15.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "concurrently": "^9.1.2",
    "eslint": "9.26.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "5.4.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "^16.1.0",
    "jest": "^29.6.2",
    "msw": "^2.8.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.0",
    "ts-jest": "29.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@commercetools-backend/loggers": "^23.3.0",
    "@commercetools/platform-sdk": "^8.9.0",
