{
  "name": "shipping-integration-ingrid",
  "version": "3.2.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node build/index.js",
    "gcp-build": "tsc",
    "start:dev": "concurrently -k \"tsc --watch\" \"nodemon -q build/index.js\"",
    "build": "rimraf ./build && tsc",
    "lint": "prettier --check \"**/**/*.{ts,js,json}\" && eslint src test",
    "prettier": "prettier --write '**/*.{js,ts}'",
    "check": "tsc --noEmit && npm run lint",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js jest --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --collectCoverageFrom='src/**/*.ts'",
    "connector:post-deploy": "node build/connector/post-deploy.js",
    "connector:pre-undeploy": "node build/connector/pre-undeploy.js"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@jest/globals": "30.0.4",
    "@tsconfig/recommended": "^1.0.10",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.15",
    "@types/supertest": "^6.0.3",
    "@types/validator": "^13.15.2",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.32.1",
    "concurrently": "^9.2.0",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "5.5.3",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "msw": "^2.10.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.3",
    "ts-jest": "29.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@commercetools-backend/loggers": "^24.2.1",
    "@commercetools/platform-sdk": "^8.12.1",
    "@commercetools/ts-client": "^4.1.0",
    "axios": "^1.11.0",
    "body-parser": "^2.2.0",
    "dotenv": "^17.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "express": "^5.1.0",
    "listr2": "^9.0.1",
    "validator": "^13.15.15"
  }
